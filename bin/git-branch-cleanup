#!/bin/bash

echo "Cleaning remote refs"
git fetch

#deletes branches that have been merged into master
branches=$(git branch --merged origin/master | tr '*' ' '| grep -v '  master$')

echo "Cleaning '$branches' merged in"
for b in "$branches"; do
  git branch -d $b
done

echo "Cleaning remote refs"
git fetch --prune

# vim: ft=sh
